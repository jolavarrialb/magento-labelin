<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="column_name">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Labelin_Sales::items/column/name.phtml</argument>
            </action>
            <block class="Labelin\Sales\Block\Adminhtml\Order\View\Column\ArtworkAcceptanceForm" name="artwork_acceptance_form"
                   template="Labelin_Sales::items/column/artworkacceptanceform.phtml" >
            </block>
            <block class="Labelin\Sales\Block\Adminhtml\Order\View\Column\Items\Renderer\DefaultRenderer\ArtWork"
                   name="labelin.sales.adminhtml.order.item.renderers.default.artwork" as="admin.artwork"
                   template="Labelin_Sales::order/items/renderer/default/artwork.phtml">
            </block>
        </referenceBlock>
    </body>
</page>
