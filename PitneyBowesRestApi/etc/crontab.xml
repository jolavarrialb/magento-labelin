<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job instance="Labelin\PitneyBowesRestApi\Cron\OauthGenerator" method="execute" name="labelin_pitneybowes_rest_api_generate_oauth">
            <schedule>0 */8 * * *</schedule>
        </job>
        <job instance="Labelin\PitneyBowesRestApi\Cron\OauthApiKeyIsValid" method="execute" name="labelin_pitneybowes_rest_api_check_actual_and_generate_oauth">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
</config>
