<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" class="Labelin\Sales\Ui\Component\MassAction">
            <action name="premade_to_production">
                <settings>
                    <type>premade_to_production</type>
                    <label translate="true">Premade to production</label>
                    <url path="production_ticket/order/premadeToProduction"/>
                    <confirm>
                        <title translate="true">Premade to production</title>
                        <message translate="true">Are you sure you need to move pre-made items from the selected orders to production?</message>
                    </confirm>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
